<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityAnalisisAdmin">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Analisis"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:textAlignment="center"
        android:layout_marginTop="356dp"/>

    <View
        android:id="@+id/penutup"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="702dp"
        android:background="@color/black"/>

    <View
        android:id="@+id/penengah"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_below="@id/penutup"
        android:background="@color/black"
        android:layout_marginStart="180dp"/>

    <ImageButton
        android:id="@+id/ambilData"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_below="@id/penutup"
        android:layout_alignParentStart="true"
        android:layout_marginStart="73dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/scan"
        android:background="@null"
        android:scaleType="fitCenter"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"/>

    <ImageButton
        android:id="@+id/analisisData"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_below="@id/penutup"
        android:layout_alignParentStart="true"
        android:layout_marginStart="260dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/analisis"
        android:background="@null"
        android:scaleType="fitCenter"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"/>

</RelativeLayout>